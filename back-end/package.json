{
  "name": "back-end-tsoa",
  "version": "1.0.0",
  "description": "Código base de back-end em TypeScript OpenAPI",
  "main": "index.js",
  "scripts": {
    "build": "npx tsoa spec-and-routes && tsc",
    "serve": "nodemon",
    "start": "node ./dist/src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migrations:generate": "npm run typeorm:cli -- migration:generate -n PostRefactoring",
    "migrations:run": "npm run typeorm:cli -- migration:run"
  },
  "author": "CompJúnior",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.14.1",
    "typeorm": "^0.2.44"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}