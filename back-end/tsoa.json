{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*.controller.ts",
    "src/**/*Controller.ts"
  ],
  "spec": {
    "basePath": "/api",
    "outputDirectory": "build",
    "specVersion": 3
  },
  "routes": {
    "basePath": "/api",
    "routesDir": "build",
    "authenticationModule": "./src/middlewares/authentication.ts"
  }
}